#include <stdio.h>
#include <stdlib.h>

struct Node {
    int data;
    struct Node* next;
};

struct Node* top = NULL;

int empty() {
    return top == NULL;
}

void push(int value) {
    struct Node* newNode = (struct Node*)malloc(sizeof(struct Node));
    if (!newNode) {
        printf("Heap overflow\n");
        return;
    }
    newNode->data = value;
    newNode->next = top; 
    top = newNode;       
}

int pop() {
    if (empty()) {
        printf("Stack Underflow\n");
        return -1;
    }
    struct Node* temp = top;
    int poppedValue = temp->data;
    top = top->next;     
    free(temp);         
    return poppedValue;
}

int peek() {
    if (empty()) return -1;
    return top->data;
}

int main() {

    push(10);
    push(20);
    push(30);

    printf("Top = %d\n", peek());

    printf("%d popped\n", pop());
    printf("%d popped\n", pop());

    return 0;
}
